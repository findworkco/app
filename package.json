    "jscs": "~3.0.7",
    "jshint": "~2.9.3",
    "karma": "~1.4.1",
    "karma-browserify": "~5.1.1",
    "karma-mocha": "~1.3.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "mocha": "~2.4.5",
    "nodemon": "~1.9.2",
    "request": "~2.72.0",
    "simulant": "~0.2.2",
    "sinon": "~1.17.6",
    "spy-server": "~1.0.0",
    "twolfson-style": "~1.6.0",
    "webdriver-manager": "~10.0.1"
  },
  "keywords": [],
  "private": true
}
