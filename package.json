{
  "name": "app",
  "description": "Web application for https://findwork.co/",
  "version": "1.0.0",
  "homepage": "https://github.com/twolfson/find-work-app",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/find-work-app.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/find-work-app/issues"
  },
  "engines": {
    "node": "4.4.1"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "gulp build-clean && gulp build",
    "develop": "gulp build-clean && gulp develop",
    "lint": "twolfson-style lint server/ test/",
    "_gemini-gui-comment": "By using `trap`, we kick off visual update script on SIGINT (keyboard interrupt)",
    "gemini-gui": "trap bin/test-visual-update-signature.sh INT; gemini-gui test/visual",
    "_gemini-update-comment": "By using `trap`, we allow additional arguments via `--` with `npm run`",
    "gemini-update": "trap bin/test-visual-update-signature.sh EXIT; npm run verify-webdriver-running && gemini update test/visual",
    "gemini-test": "trap bin/test-visual-update-signature.sh EXIT; npm run verify-webdriver-running && gemini test --reporter html --reporter flat test/visual",
    "migrate-latest": "sequelize db:migrate",
    "migrate-undo": "sequelize db:migrate:undo",
    "migrate-undo-all": "sequelize db:migrate:undo:all",
    "postinstall": "bower install && (npm run webdriver-update || true) && npm run build",
    "start": "npm run start-server & npm run start-queue",
    "start-develop": "npm run start-server-develop & npm run start-queue-develop",
    "start-queue": "node bin/find-work-queue",
    "start-queue-develop": "ENV=development nodemon --exec bin/find-work-queue --ignore dist --ignore public",
    "start-server": "node bin/find-work-server",
    "start-server-develop": "ENV=development nodemon --exec bin/find-work-server --ignore dist --ignore public",
    "start-test-redis": "bin/start-test-redis.sh",
    "test": "npm run lint && npm run start-test-redis && npm run test-server && npm run verify-images-optimized",
    "test-server": "ENV=test KUE=1 mocha --timeout 5000 --recursive test/server/utils/test.js test/server",
    "verify-images-optimized": "test/verify-images-optimized.sh",
    "verify-webdriver-running": "curl --silent http://localhost:4444/ > /dev/null || (echo \"Webdriver isn't running, please start it via \\`npm run webdriver-start\\`\" 1>&2 && exit 1)",
    "webdriver-start": "xvfb-run webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "alertify.js": "~1.0.11",
    "async": "~2.1.2",
    "body-parser-multidict": "~1.1.0",
    "connect-ensure-login": "~0.1.1",
    "connect-flash": "~0.1.1",
    "connect-redis": "~3.0.2",
    "country-data": "0.0.24",
    "csurf": "~1.9.0",
    "express": "~4.13.4",
    "express-session": "~1.13.0",
    "extract-values": "~0.1.2",
    "gravatar-url": "~1.1.0",
    "http-errors": "~1.5.0",
    "inflection": "~1.10.0",
    "jade": "~1.11.0",
    "json-stringify-safe": "~5.0.1",
    "kue": "~0.11.5",
    "methods": "~1.1.2",
    "moment": "~2.12.0",
    "moment-timezone": "~0.5.3",
    "multiline": "~1.0.2",
    "nodemailer": "~2.6.4",
    "nodemailer-html-to-text": "~2.1.0",
    "passport": "~0.3.2",
    "passport-google-oauth20": "~1.0.0",
    "pg": "~6.1.0",
    "querystring-multidict": "~1.1.0",
    "raven": "~0.12.1",
    "redis": "~2.6.0-2",
    "sanitize-html": "~1.11.4",
    "sequelize": "~3.24.3",
    "shallow-settings": "~0.1.0",
    "statuses": "~1.3.0",
    "underscore": "~1.8.3"
  },
  "optionalDependencies": {
    "autosize-input": "git://github.com/twolfson/autosize-input.git#385931f",
    "bootstrap-datepicker": "~1.6.0",
    "bower": "~1.7.7",
    "browserify": "~13.0.0",
    "browsernizr": "~2.1.0",
    "gulp": "~3.9.1",
    "gulp-buffer": "0.0.2",
    "gulp-csso": "~1.1.0",
    "gulp-imagemin": "~2.4.0",
    "gulp-livereload": "~3.8.1",
    "gulp-notify": "~2.2.0",
    "gulp-sass": "~2.2.0",
    "gulp-sizereport": "~1.1.3",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-svgmin": "~1.2.2",
    "gulp-uglify": "~1.5.3",
    "jquery": "~2.2.3",
    "rimraf": "~2.5.2",
    "sequelize-cli": "git://github.com/twolfson/sequelize-cli.git#8cb60ca",
    "timepicker": "~1.8.11",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
