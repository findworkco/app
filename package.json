{
  "name": "app",
  "description": "Web application for https://findwork.co/",
  "version": "1.0.0",
  "homepage": "https://github.com/twolfson/find-work-app",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/find-work-app.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/find-work-app/issues"
  },
  "engines": {
    "node": "4.4.1"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "gulp build-clean && gulp build",
    "develop": "gulp build-clean && gulp develop",
    "lint": "twolfson-style lint server/ test/",
    "gemini-gui": "gemini-gui test/visual-tests",
    "gemini-update": "npm run verify-webdriver-running && gemini update test/visual-tests",
    "gemini-test": "npm run verify-webdriver-running && gemini test --reporter html --reporter flat test/visual-tests",
    "postinstall": "bower install && npm run webdriver-update && npm run build",
    "start": "serve -p 9000",
    "test": "npm run lint && npm run verify-images-optimized",
    "verify-images-optimized": "test/verify-images-optimized.sh",
    "verify-webdriver-running": "curl --silent http://localhost:4444/ > /dev/null || (echo \"Webdriver isn't running, please start it via \\`npm run webdriver-start\\`\" 1>&2 && exit 1)",
    "webdriver-start": "xvfb-run webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "moment": "~2.12.0",
    "moment-timezone": "~0.5.3"
  },
  "devDependencies": {
    "bootstrap-datepicker": "~1.6.0",
    "bower": "~1.7.7",
    "browserify": "~13.0.0",
    "gemini": "~3.0.2",
    "gemini-gui": "~3.0.0",
    "gulp": "~3.9.1",
    "gulp-buffer": "0.0.2",
    "gulp-csso": "~1.1.0",
    "gulp-livereload": "~3.8.1",
    "gulp-notify": "~2.2.0",
    "gulp-sass": "~2.2.0",
    "gulp-sizereport": "~1.1.3",
    "gulp-svgmin": "~1.2.2",
    "gulp-uglify": "~1.5.3",
    "jquery": "~2.2.3",
    "jscs": "~1.7.3",
    "jshint": "~2.5.10",
    "mocha": "~2.4.5",
    "rimraf": "~2.5.2",
    "serve": "git://github.com/twolfson/serve#f094470",
    "twolfson-style": "~1.6.0",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.7.0",
    "webdriver-manager": "~9.0.0"
  },
  "keywords": [],
  "private": true
}
